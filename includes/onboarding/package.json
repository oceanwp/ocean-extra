  },
  "devDependencies": {
    "@wordpress/block-editor": "^14.15.0",
    "@wordpress/components": "^29.4.0",
    "@wordpress/element": "^6.18.0",
    "@wordpress/hooks": "^4.18.0",
    "@wordpress/i18n": "^5.18.0",
    "@wordpress/scripts": "^30.11.0",
    "prettier": "^3.5.2"
  }
}
